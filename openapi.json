              }
            }
          }
        }
      }
    },
    "/courses/can-take/{course_code}": {
      "post": {
        "summary": "Courses Can Take",
        "operationId": "courses_can_take_courses_can_take__course_code__post",
        "parameters": [
          {
            "name": "course_code",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "title": "Course Code" }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/CoursesTakenIn" }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/RequirementsResults" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/courses/search": {
      "get": {
        "summary": "Search Courses",
        "operationId": "search_courses_courses_search_get",
        "parameters": [
          {
            "name": "degree_name",
            "in": "query",
            "required": true,
            "schema": { "type": "string", "title": "Degree Name" }
          },
          {
            "name": "degree_year",
            "in": "query",
            "required": true,
            "schema": { "type": "integer", "title": "Degree Year" }
          },
          {
            "name": "q",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [{ "type": "string" }, { "type": "null" }],
              "title": "Q"
            }
          },
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                { "type": "integer", "minimum": 0 },
                { "type": "null" }
              ],
              "title": "Number of courses to offset the results from",
              "default": 0
            }
          },
          {
            "name": "page_size",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                { "type": "integer", "maximum": 100, "exclusiveMinimum": 0 },
                { "type": "null" }
              ],
              "title": "Number of results returned",
              "default": 20
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CourseWithTagsSchema"
                  },
                  "title": "Response Search Courses Courses Search Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/courses/tags": {
      "get": {
        "summary": "Tags",
        "operationId": "tags_courses_tags_get",
        "parameters": [
          {
            "name": "degree_name",
            "in": "query",
            "required": true,
            "schema": { "type": "string", "title": "Degree Name" }
          },
          {
            "name": "degree_year",
            "in": "query",
            "required": true,
            "schema": { "type": "string", "title": "Degree Year" }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CourseWithTagsSchema"
                  },
                  "title": "Response Tags Courses Tags Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/sample-path": {
      "get": {
        "summary": "Sample Path",
        "operationId": "sample_path_sample_path_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AdditionalReqCount": {
        "properties": {
          "completed": { "type": "string", "title": "Completed" },
          "total": { "type": "string", "title": "Total" }
        },
        "type": "object",
        "required": ["completed", "total"],
        "title": "AdditionalReqCount"
      },
      "ColorsEnum": {
        "type": "string",
        "enum": [
          "red",
          "green",
          "orange",
          "yellow",
          "blue",
          "purple",
          "pink",
          "indigo",
          "gray"
        ],
        "title": "ColorsEnum"
      },
      "CourseWithTagsSchema": {
        "properties": {
          "courseCode": { "type": "string", "title": "Coursecode" },
          "courseName": { "type": "string", "title": "Coursename" },
          "credit": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Credit",
            "default": -1000
          },
          "description": {
            "type": "string",
            "title": "Description",
            "default": ""
          },
          "location": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Location",
            "default": ""
          },
          "prerequisites": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Prerequisites",
            "default": ""
          },
          "antirequisites": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Antirequisites",
            "default": ""
          },
          "corequisites": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Corequisites",
            "default": ""
          },
          "tags": {
            "items": { "$ref": "#/components/schemas/TagSchema" },
            "type": "array",
            "title": "Tags",
            "default": []
          }
        },
        "type": "object",
        "required": ["courseCode", "courseName"],
        "title": "CourseWithTagsSchema"
      },
      "CoursesTakenIn": {
        "properties": {
          "courseCodesTaken": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Coursecodestaken"
          }
        },
        "type": "object",
        "required": ["courseCodesTaken"],
        "title": "CoursesTakenIn"
      },
      "DegreeMissingIn": {
        "properties": {
          "courseCodesTaken": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Coursecodestaken"
          },
          "year": { "type": "string", "title": "Year" }
        },
        "type": "object",
        "required": ["courseCodesTaken", "year"],
        "title": "DegreeMissingIn"
      },
      "DegreeMissingReqs": {
        "properties": {
          "mandatoryCourses": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Mandatorycourses"
          },
          "additionalReqs": {
            "additionalProperties": {
              "$ref": "#/components/schemas/AdditionalReqCount"
            },
            "type": "object",
            "title": "Additionalreqs"
          }
        },
        "type": "object",
        "required": ["mandatoryCourses", "additionalReqs"],
        "title": "DegreeMissingReqs"
      },
      "DegreeReqs": {
        "properties": {
          "mandatoryCourses": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Mandatorycourses"
          },
          "additionalReqs": {
            "additionalProperties": {
              "$ref": "#/components/schemas/DegreeRequirement"
            },
            "type": "object",
            "title": "Additionalreqs"
          }
        },
        "type": "object",
        "required": ["mandatoryCourses", "additionalReqs"],
        "title": "DegreeReqs"
      },
      "DegreeRequirement": {
        "properties": {
          "courses": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Courses"
          },
          "numberOfCourses": { "type": "integer", "title": "Numberofcourses" }
        },
        "type": "object",
        "required": ["courses", "numberOfCourses"],
        "title": "DegreeRequirement"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "OptionRequirement": {
        "properties": {
          "courses": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Courses"
          },
          "numberOfCourses": { "type": "integer", "title": "Numberofcourses" }
        },
        "type": "object",
        "required": ["courses", "numberOfCourses"],
        "title": "OptionRequirement"
      },
      "OptionsSchema": {
        "properties": {
          "optionName": { "type": "string", "title": "Optionname" },
          "requirements": {
            "items": { "$ref": "#/components/schemas/OptionRequirement" },
            "type": "array",
            "title": "Requirements"
          }
        },
        "type": "object",
        "required": ["optionName", "requirements"],
        "title": "OptionsSchema"
      },
      "RequirementsResults": {
        "properties": {
          "result": { "type": "boolean", "title": "Result" },
          "message": { "type": "string", "title": "Message" }
        },
        "type": "object",
        "required": ["result", "message"],
        "title": "RequirementsResults"
      },
      "TagSchema": {
        "properties": {
          "code": { "type": "string", "title": "Code" },
          "color": {
            "allOf": [{ "$ref": "#/components/schemas/ColorsEnum" }],
            "default": "red"
          },
          "shortName": { "type": "string", "title": "Shortname" },
          "longName": { "type": "string", "title": "Longname" }
        },
        "type": "object",
        "required": ["code", "shortName", "longName"],
        "title": "TagSchema"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
